        ( maybe mempty (Data.Aeson.Encoding.pair "name" . Data.Aeson.toEncoding) name
        )

instance Data.Aeson.FromJSON Package where
    parseJSON = Data.Aeson.withObject "Package" $ \o ->
        Package
            <$> o Data.Aeson..:? "name"
